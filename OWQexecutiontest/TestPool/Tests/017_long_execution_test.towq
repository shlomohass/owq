let str = "abcdefgh" + "efghefgh";
let imax = 1024 / str.length() * 4 * 512 * 2; // 262144

let i = 0;
let lngth;

// will loop 65536 times:
while (i < imax) {
    lngth = str.length() * i;
	i = i + 1;
}

print("Iterations: " + i + " - Result: " + lngth);

/*
@expected-output
Iterations: 262144 - Result: 4194288
@end-expected-output
*/

let a, b, c, d;
a = "owq";
b -> a;
c -> b;

print(a.isPointer() + ","); // FALSE
print(a.isPointed() + ","); // TRUE
print(b.isPointer() + ","); // TRUE
print(b.isPointed() + ","); // TRUE
